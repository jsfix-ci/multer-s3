{
  "name": "veritas-s3-transform",
  "version": "3.1.1",
  "description": "Streaming multer storage engine for AWS S3",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/basic.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AssetVal/multer-s3.git"
  },
  "keywords": [
    "multer",
    "s3",
    "amazon",
    "sharp",
    "aws"
  ],
  "author": "Antonio Bourassa (Abourassa@assetval.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AssetVal/multer-s3/issues"
  },
  "homepage": "https://github.com/AssetVal/multer-s3#readme",
  "dependencies": {
    "file-type": "^16.1.0",
    "is-svg": "^4.2.2",
    "run-parallel": "^1.1.6"
  },
  "devDependencies": {
    "@getify/eslint-plugin-proper-arrows": "^10.0.0",
    "@getify/eslint-plugin-proper-ternary": "^3.0.1",
    "@types/multer": "^1.4.5",
    "@types/multer-s3": "^2.7.9",
    "@types/node": "^14.14.19",
    "@types/run-parallel": "^1.1.0",
    "aws-sdk": "^2.820.0",
    "concat-stream": "^2.0.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.4.0",
    "express": "^4.13.1",
    "express-serve-static-core": "^0.1.1",
    "form-data": "^3.0.0",
    "mocha": "^8.2.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "on-finished": "^2.3.0",
    "xtend": "^4.0.1"
  },
  "eslintConfig": {
    "plugins": [
      "node",
      "security",
      "@getify/proper-ternary"
    ],
    "extends": [
      "airbnb-base",
      "plugin:node/recommended",
      "plugin:security/recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:@getify/proper-ternary/getify-says"
    ],
    "env": {
      "shared-node-browser": true,
      "es6": true,
      "mongo": true,
      "jquery": true
    },
    "rules": {
      "node/no-unsupported-features/es-syntax": [
        "error",
        {
          "ignores": [
            "dynamicImport",
            "modules"
          ]
        }
      ],
      "arrow-parens": [
        "error",
        "as-needed",
        {
          "requireForBlockBody": true
        }
      ],
      "one-var": "off",
      "class-methods-use-this": "off",
      "one-var-declaration-per-line": "off",
      "consistent-return": "off",
      "func-names": "off",
      "max-len": "off",
      "no-unused-expressions": "off",
      "no-console": "off",
      "no-shadow": [
        "error",
        {
          "hoist": "never"
        }
      ],
      "no-param-reassign": "off",
      "no-prototype-builtins": "off",
      "no-new": "off",
      "newline-per-chained-call": "off",
      "no-lonely-if": "error",
      "no-new-object": "error",
      "no-plusplus": "off",
      "no-bitwise": "off",
      "object-curly-newline": [
        "error",
        {
          "multiline": true,
          "consistent": true
        }
      ],
      "no-underscore-dangle": "off",
      "object-curly-spacing": [
        "error",
        "never"
      ],
      "object-shorthand": [
        "error",
        "consistent"
      ],
      "prefer-destructuring": "off",
      "space-before-blocks": "off",
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "prefer-rest-params": "off",
      "no-undef": "off",
      "lines-between-class-members": "off",
      "camelcase": "off",
      "import/newline-after-import": "off",
      "import/no-named-as-default": "off",
      "import/extensions": "off",
      "import/no-unresolved": "off",
      "security/detect-object-injection": "off",
      "@getify/proper-ternary/where": "off"
    }
  }
}
